{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=DocHelper;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Caching": {
    "UseCache": true,
    "Provider": "InMemory",
    "Timeout": 20,
    
    "MaxRdSecond": 60,
    "EnableLogging": false,
    "SleepMs": 300,
    "LockMs": 5000
  },
"AllowedHosts": "*"
}
